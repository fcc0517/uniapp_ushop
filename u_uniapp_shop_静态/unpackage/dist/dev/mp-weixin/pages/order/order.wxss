
.search{
		margin: 50rpx;
		height: 80rpx;
		background-color: #f26b11;
		border-radius: 80rpx;
		color: white;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
icon{
		margin:0 10rpx 0 250rpx;
}
.placeholder{
		color: white;
		font-size: 25rpx;
}
.row{
		margin: 10rpx 20rpx;
		/* height: 240rpx; */
		border: 1rpx solid #AAAAAA;
		border-radius: 30rpx;
}
.imagebox{
		/* float: left; */
		margin:10rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
}
.imagebox .title{
		margin-top: 30rpx;
}
.products{
		overflow: hidden;
}
image{
		width: 160rpx;
		margin-left: 10rpx;
}
.info{
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
		font-size: 25rpx;
		/* width: 340rpx; */
		padding: 20rpx;
		text-align: right;
}
.info text:nth-of-type(2){
		color: red;
}
.info text:nth-of-type(3){
		font-size: 20rpx;
		color: #AAAAAA;
}
.tishi{
		line-height: 200rpx;
		text-align: center;
		font-size: 40rpx;
		color: #006699;
}

